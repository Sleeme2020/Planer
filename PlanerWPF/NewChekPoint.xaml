<Window x:Class="PlanerWPF.NewChekPoint"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PlanerWPF"
        mc:Ignorable="d"
        Title="NewChekPoint" Height="241" Width="429">
    <Grid>
        <TextBox x:Name="tb" HorizontalAlignment="Left" Margin="65,37,0,0" TextWrapping="Wrap" Text="{Binding NameChekPoint}" VerticalAlignment="Top" Width="305" Height="25"/>
        <DatePicker SelectedDate="{Binding Date}" HorizontalAlignment="Left" Margin="65,87,0,0" VerticalAlignment="Top" Height="36" Width="305"/>
        <Button Command="{Binding Saved}" Content="Button" HorizontalAlignment="Left" Margin="321,171,0,0" VerticalAlignment="Top" Height="34" Width="89" Click="Button_Click"/>

    </Grid>
</Window>
